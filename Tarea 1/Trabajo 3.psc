Algoritmo Division_Mayor
	Definir Num1, Num2 Como Entero;
	Definir RR Como Real;
	Escribir "Introduce los numeros";
	Escribir "Valor 1";
	Leer Num1;
	Escribir "Valor 2";
	Leer Num2;
	Si Num1>Num2 Entonces;
		RR=Num1/Num2;
	SiNo
		RR=Num1/Num2;
	FinSi
	Escribir "La division es=", RR;
FinAlgoritmo
